#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int limite;
    cin >> limite; 
    int arr[limite];
    for(int i = 0; i < limite; i++){
        cin >> arr[i];
    }
    for(int i = 0; i < limite; i++){
        cout << arr[limite-1-i] << " ";
    }
      
    return 0;
}
