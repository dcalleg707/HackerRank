nums = input().split(" ")
N, M = int(nums[0]), int(nums[1])
for i in range(N//2):
    print("-"*((M//2)-1 - 3*(i)) + ".|."*(1+(2*i)) + "-"*((M//2)-1 - 3*(i)))
print("-"*((M-7)//2) + "WELCOME" + "-"*((M-7)//2))
for i in range(N//2-1,-1,-1):
    print("-"*((M//2)-1 - 3*(i)) + ".|."*(1+(2*i)) + "-"*((M//2)-1 - 3*(i)))
