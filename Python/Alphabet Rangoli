def print_rangoli(size):
    if size == 1:
        print("a")
        return 0
    a = list(map(chr, list(range(97,97+size))))
    patron = [(("-".join(a[len(a)-1:len(a)-i-1:-1]+a[len(a)-i-1:]))).center((size-1)*4+1, "-") for i in range(size-1)]
    print("\n".join(patron) +"\n" + "-".join(a[::-1] + a[1:]) + "\n"+"\n".join(patron[::-1]))
